{%  extends 'base.html.twig' %}

{%  block body %}
<h1>Inscription sur le site</h1>

{{ form_start(form) }}
{{ form_row(form.nom) }}
{{ form_row(form.prenom) }}
{{ form_row(form.email) }}
{{ form_row(form.motDePasse) }}
{{ form_row(form.confirm_motDePasse,{ 'label' : 'Confirmation du mot de passe'}) }}

<button type="submit" class="btn btn-success">Inscription ! </button>
{{ form_end(form)}}
{% endblock %}